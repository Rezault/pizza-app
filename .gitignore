# ignore python cache files
__pycache__/
*.pyc
*.pyo
*.pyd

# ignore the SQLite database file
db.sqlite3
*.sqlite3

# ignore django migration files (optional, but recommended for cleaner commits)
**/migrations/*.pyc
**/migrations/*.py
!**/migrations/__init__.py  # Keep the __init__.py so Django recognizes migrations folder

# ignore environment variables and secret keys (for later)
.env
*.env

# ignore static files (they should be generated, not stored in Git)
staticfiles/
media/
static/

# ignore compiled CSS (if using SASS or LESS)
*.sass-cache
*.scssc
*.css.map

# ignore VS Code settings
.vscode/
